{
  "label": "$P",
  "name": "pdollar-recognizer",
  "type": "recognizer",
  "description": "TODO.",
  "properties": {},
  "settings": [
    {
      "type": "setting",
      "label": "Number of sampling points",
      "name": "samplingPoints",
      "description": "The number of points of the gesture after resampling.",
      "data": {
        "category": "item",
        "dataType": "integer",
        "domain": {
          "type": "range",
          "low": 2,
          "high": null
        },
        "default": 16
      }
    },
    {
      "type": "setting",
      "label": "Path",
      "name": "pathName",
      "description": "The path that is used by the recognizer.",
      "data": {
        "category": "item",
        "dataType": "string",
        "domain": {
          "type": "list",
          "values": {
            "uri": "/configurations/current/modules/sensor/properties?property=points"
          }
        },
        "default": []
      }
    }
  ]
}